#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.9_@types+node@20.12.12_sass@1.77.1__vue@3.4.27/node_modules/vuepress/bin/node_modules:/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.9_@types+node@20.12.12_sass@1.77.1__vue@3.4.27/node_modules/vuepress/node_modules:/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.9_@types+node@20.12.12_sass@1.77.1__vue@3.4.27/node_modules:/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.9_@types+node@20.12.12_sass@1.77.1__vue@3.4.27/node_modules/vuepress/bin/node_modules:/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.9_@types+node@20.12.12_sass@1.77.1__vue@3.4.27/node_modules/vuepress/node_modules:/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/vuepress@2.0.0-rc.9_@vuepress+bundler-vite@2.0.0-rc.9_@types+node@20.12.12_sass@1.77.1__vue@3.4.27/node_modules:/Users/yanwei/Downloads/HarmonyOS-Guide/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress-vite.js" "$@"
fi
